<p id="to-top" style="display:none;">
    <a href="#top"><span><span></span></span>{# Вверх #}</a>
</p>
<script>
$(function(){
    $(window).scroll(function(){
        if($(this).scrollTop()>300){
            $('p#to-top').fadeIn();
            if(callback==false){
                callback=true;
                setTimeout(CallBack_Enable, 65000)
            }
        }else{
            $('p#to-top').fadeOut();
        }
    });
    $('p#to-top a').click(function(){
        $('body,html').animate({
            scrollTop:15
        }, 2500);
        return false;
    });
});
</script>
