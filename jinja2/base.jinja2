<script>
function cachedScript(url,done_url,options){
options=$.extend(options||{},{dataType:"script",cache:true});
if(done_url){
    return(
            $.ajax(
                    {
                        url:url,
                        dataType:"script",
                        cache:true,
                        async:false,
                        success:function()
                        {
                            $.ajax(
                                    {
                                        url:done_url,
                                        timeout:1000,
                                        dataType:"script",
                                        cache:true,
                                        async:false
                                    }
                            )
                        }
                    }
            )
    )
}
else{return $.ajax($.extend(options||{},{url:url}));}}</script>
