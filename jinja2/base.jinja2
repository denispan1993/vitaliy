                    document.getElementById('feedback-panel').style.display="none";
                    alert('Ваши данные не принял сервер по причине: '+data.error);
                }else{
                    document.getElementById('feedback-panel').style.display="none";
                    alert('Мы рассмотрим Ваше замечание/пожелание, в ближайшее время и свяжемся с Вами !!!');
